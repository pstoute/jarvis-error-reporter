{
    "name": "pstoute/jarvis-error-reporter",
    "description": "Send Laravel errors to Jarvis for automatic analysis and fixing",
    "keywords": ["laravel", "error-reporting", "jarvis", "autofix"],
    "license": "MIT",
    "authors": [
        {
            "name": "Paul Stoute",
            "email": "paul@stoutewebsolutions.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "StouteWebSolutions\JarvisErrorReporter\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "StouteWebSolutions\JarvisErrorReporter\JarvisServiceProvider"
            ],
            "aliases": {
                "Jarvis": "StouteWebSolutions\JarvisErrorReporter\Facades\Jarvis"
            }
        }
    },
    "minimum-stability": "stable"
}
